{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"taskName": "clisp (load vrp file into clisp)",
			"type": "shell",
			"command": "clisp -i -q vrp.lisp",
			"problemMatcher": [],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"taskName": "run tests",
			"type": "shell",
			"command": "clisp -q test_vrp.lisp",
			"problemMatcher": [],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		}
	]
}